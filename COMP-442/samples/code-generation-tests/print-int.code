int to_upper_caps(int c){

	int greater;
	int lesser;
	
	if(c >= 97)
	then{
		greater = 1;
	}else{
		greater = 0;
	};
	
	if(c <= 122)
	then{
		lesser = 1;
	}else{
		lesser = 0;
	};
	
	if(lesser and greater)
	then{
		return(c - 32);
	}else{
		return(c);
	};

};